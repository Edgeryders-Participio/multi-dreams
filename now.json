{
  "version": 2,
  "builds": [
    { "src": "ui/next.config.js", "use": "@now/next" },
    { "src": "api/index.js", "use": "@now/node" },
    { "src": "api/webhooks/pretix.js", "use": "@now/node" }
  ],
  "routes": [
    { "src": "/api", "dest": "/api/index.js" },
    { "src": "/api/pretix", "dest": "/api/webhooks/pretix.js" },
    { "src": "/(.*)", "dest": "/ui/$1", "continue": true }
  ],
  "regions": ["bru1"]
}
